{
  "name": "claude-command-center",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bash scripts/dev.sh",
    "dev:server": "tsx watch server/index.ts",
    "postinstall": "chmod +x node_modules/node-pty/prebuilds/darwin-arm64/spawn-helper 2>/dev/null || true",
    "build": "cd frontend && npm run build",
    "start": "node --import tsx server/index.ts"
  },
  "dependencies": {
    "better-sqlite3": "^11.7.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.4",
    "express": "^4.21.0",
    "jose": "^5.9.0",
    "nanoid": "^5.0.9",
    "node-pty": "^1.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.13",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
